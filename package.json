{
  "name": "advent-of-code-js",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "export-code": "copyfiles --verbose --up 2 \"app/solutions/*/*/solution-part*.ts\" app/text-content/files",
    "export-type": "copyfiles --verbose --up 2 \"app/solutions/*/*/types.ts\" app/text-content/files",
    "preexport": "rimraf app/text-content/files",
    "export": "npm run export-code && npm run export-type",
    "postexport": "renamer --path-element ext \"app/text-content/files/**\" --find .ts --replace .txt --force",
    "dev": "next dev --turbopack",
    "debug": "cross-env NODE_OPTIONS=--inspect next dev --turbopack",
    "prebuild": "npm run export",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "clsx": "^2.1.1",
    "highlight.js": "^11.11.1",
    "next": "15.1.4",
    "next-auth": "^5.0.0-beta.25",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.1.4",
    "postcss": "^8",
    "renamer": "^5.0.2",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
